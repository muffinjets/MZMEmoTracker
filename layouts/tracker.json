{
    "item_grid": {
        "type": "itemgrid",
        "h_alignment": "stretch",
        "item_margin": "4,4",
        "rows": [
            ["etanks","missile","supers","powerbomb"],
            ["morph","bomb","varia","gravity"],
            ["long","charge","ice","wave","plasma"],
            ["grip","boots","speed","screw","space"]
        ]
    },
    "boss_grid": {
        "type": "itemgrid",
        "h_alignment": "stretch",
        "item_margin": "4,4",
        "rows": [
            ["acidwormitem"],
            ["kraiditem"],
            ["ridleyitem"],
            ["charlieitem"]
        ]
    },
    "settings_grid": {
