    "tracker_default": {
        "type": "container",
        "background": "#28622e",
        "content": {
            "type": "dock",
            "dropshadow": false,
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "v_alignment": "stretch",
                    "margin": "0,0,3,0",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "top",
                            "header_content": {
                                "type": "button_popup",
                                "style": "settings",
                                "popup_background": "#50212121",
                                "layout": "settings_grid"
                            },
                            "content": {
                                "type": "layout",
                                "key": "item_grid"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Pinnned Locations",
